{
  "name": "discordjs-typescript-template",
  "version": "1.0.0",
  "main": "src/bot.ts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "ts-node src/scripts/start.ts",
    "test": "npx jest",
    "test:watch": "npx jest --watch",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && yarn lint && yarn test",
    "dev": "nodemon --exec ts-node src/scripts/start.ts",
    "deploy-commands": "ts-node src/scripts/deployCommands.ts",
    "delete-commands": "ts-node src/scripts/deleteCommands.ts"
  },
  "dependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ical-expander": "^3.1.0",
    "luxon": "^3.3.0",
    "mysql": "^2.18.1",
    "node-ical": "^0.16.1",
    "rcon-client": "^4.2.3",
    "rrule": "^2.7.2",
    "yesno": "^0.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/luxon": "^3.3.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^20.1.2",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@typescript-eslint/parser": "^5.59.5",
    "eslint": "^8.40.0",
    "jest": "^29.6.1",
    "jest-mock-extended": "^3.0.4",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
